{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "diagnostics": {
    "disable": [
      "close-non-object",
      "incomplete-signature-doc",
      "missing-global-doc",
      "missing-local-export-doc",
      "not-yieldable"
    ],
    "groupFileStatus": {
      "ambiguity": "Any",
      "await": "Any",
      "duplicate": "Any",
      "global": "Any",
      "luadoc": "Any",
      "redefined": "Any",
      "strict": "Any",
      "type-check": "Any",
      "unbalanced": "Any",
      "unused": "Any"
    },
    "ignoredFiles": "Opened",
    "libraryFiles": "Opened",
    "unusedLocalExclude": [
      "_*"
    ],
    "globals": [
      "vim",
      "describe",
      "it",
      "assert"
    ]
  },
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "maxPreload": 1600,
    "preloadFileSize": 10000,
    "checkThirdParty": true,
    "ignoreDir": [
      ".old"
    ],
    "library": [
      "./lua",
      "/nix/store/5rnsqz84mlj0d1rrfnpbphmwramcs6gj-neovim-0.11.2/share/nvim/runtime/lua",
      "/nix/store/y78ri890hql30kcyrwa0688fs8r0k0g4-vimplugin-luajit2.1-nvim-nio-1.10.1-1-unstable-1.10.1-1/lua",
      "/nix/store/7w7pggh8vz5fx6daamb42q84ny14175a-luajit2.1-busted-2.2.0-1/share/lua/5.1"
    ]
  },
  "typeFormat": {
    "config": {
      "format_line": "false"
    }
  },
  "type": {
    "castNumberToInteger": false,
    "checkTableShape": true,
    "inferParamType": true,
    "weakNilCheck": false,
    "weakUnionCheck": false
  },
  "doc": {
    "privateName": [
      "_*"
    ]
  },
  "strict": {
    "typeCall": true
  }
}